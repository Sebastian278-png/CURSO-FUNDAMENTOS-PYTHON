n = int(input("¿Cuántos elementos desea ingresar en la lista? "))
lista = []
for i in range(n):
  elemento = input(f"Ingrese el elemento {i+1}: ")
  lista.append(elemento)

print("Contenido de la lista:", lista)


numeros = [1, 2, 3, 4, 2, 5, 6, 2, 7, 8, 9, 2]

print("Lista original:", numeros)


num_eliminar = int(input("Ingrese el número que desea eliminar: "))


numeros = [num for num in numeros if num != num_eliminar]

print("Lista después de eliminar el número:", numeros)

print("Contenido de la lista invertido:", lista[::-1])


frase1 = input("Ingrese la primera frase: ")
frase2 = input("Ingrese la segunda frase: ")
33333

longitud_minima = min(len(frase1), len(frase2))


letras_comunes = [frase1[i] for i in range(longitud_minima) if frase1[i] == frase2[i]]


print("Letras repetidas en la misma posición:", letras_comunes)
